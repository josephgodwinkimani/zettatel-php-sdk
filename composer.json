{
    "name": "gkimani/zettatel-php-sdk",
    "description": "Unofficial PHP SDK for Zettatel SMS API",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "2.5.0",
        "php": "^7.4 || ^8.1 || ^8.2 || ^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "romanzipp/php-cs-fixer-config": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gkimani\\ZettatelPhpSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "josephgodwinkimani",
            "email": "josephgodwinke@yandex.com"
        }
    ],
    "minimum-stability": "stable"
